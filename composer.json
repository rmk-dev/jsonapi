{
    "name": "rmk/jsonapi",
    "description": "Simple JSON API implementation",
    "require": {
        "rmk/collections": "^1.0",
        "psr/container": "^2.0",
        "php": "^7.4 || ^8.0",
        "ext-json": "*",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/http-factory": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "http-interop/http-factory-guzzle": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Rmk\\JsonApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RmkTests\\JsonApi\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Kiril Savchev",
            "email": "k.savchev@gmail.com"
        }
    ]
}
